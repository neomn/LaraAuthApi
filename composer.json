{
    "name": "neomn/laraauthapi",
    "description": "api authentication",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",

    "autoload": {
        "psr-4": {
            "neomn\\laraauthapi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "neomn\\laraauthapi\\tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "neomn\\laraauthapi\\LaraAuthApiServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "6.0",
        "nunomaduro/collision": "^5.10"
    }
}
